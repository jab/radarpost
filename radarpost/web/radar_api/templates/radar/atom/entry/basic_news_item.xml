{% load radar_feed %}
<entry>
    {% block entry_body %}
    <id>{{id}}</id>
    <title type="html">
        <![CDATA[{{title}}]]>
    </title>
    <updated>{{ timestamp|rfc3339 }}</updated>
    
    <link rel="alternate" href="{{link}}" />
    
    <author>
        <name>{{ author }}</name>
    </author>

    <content type="html">
        <![CDATA[{{ content }}]]>    
    </content>
    {% endblock %}
</entry>